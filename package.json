    "eslint": "9.12.0",
    "eslint-config-prettier": "9.1.0",
    "mini-css-extract-plugin": "2.9.1",
    "postcss": "8.4.47",
    "postcss-loader": "8.1.1",
    "prettier": "3.3.3",
    "sass": "1.79.5",
    "sass-loader": "16.0.2",
    "stylelint": "16.10.0",
    "stylelint-config-sass-guidelines": "12.1.0",
    "stylelint-order": "6.0.4",
    "the-new-css-reset": "1.11.3",
    "ts-loader": "9.5.1",
    "ts-node": "10.9.2",
